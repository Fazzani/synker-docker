{
  "description": "sitepack.ini webgrab",
  "processors": [{
      "grok": {
        "field": "message",
        "patterns": [
          "<channel update=\"i\" site=\"%{DATA:site}\" site_id=\"%{DATA:site_id}\" xmltv_id=\"%{DATA:xmltv_id}\">%{DATA:channel_name}</channel>"
        ],
        "ignore_missing": true,
        "ignore_failure": true
      }
    },
    {
      "set": {
        "field": "_id",
        "value": "{{_source.site_id}}-_-{{_source.xmltv_id}}"
      }
    },
    {
      "set": {
        "field": "update_date",
        "value": "{{_ingest.timestamp}}"
      }
    },
    {
      "script": {
        "source": "if(ctx.source != null) { ctx.country =  /\\//.split(ctx.source)[4]; } else { ctx.country = '' }"
      }
    }, {
      "script": {
        "source": "if(ctx.displayNames == null) { ctx.displayNames =  [ ctx.channel_name ]; }"
      }
    }
  ]
}